{
  "name": "bucket-list",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "ulimit -n 10240 && npx expo start -c",
    "android": "ulimit -n 10240 && npx expo start --android -c",
    "ios": "ulimit -n 10240 && npx expo start --ios -c --localhost",
    "web": "npx expo start --web",
    "clear-cache": "npx expo start -c",
    "fix-deps": "npx expo install --fix",
    "setup": "npm run fix-deps && npm run clear-cache"
  },
  "dependencies": {
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.11.0",
    "expo": "~49.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.72.10",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-reanimated": "~3.3.0",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-web": "~0.19.6",
    "typescript": "^5.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.2.14"
  },
  "private": true
}
